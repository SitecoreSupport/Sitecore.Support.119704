<?xml version="1.0" encoding="utf-8" ?>
<!-- Save dialog when inserting into bucket fix  -->

<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <commands>
      <command patch:instead="*[@name='item:addmaster']" name="item:addmaster" type="Sitecore.Shell.Framework.Commands.AddMaster,Sitecore.Kernel" />
    </commands>

    <processors>
      <uiAddFromTemplate>
        <processor patch:instead="*[@type='Sitecore.Buckets.Pipelines.UI.AddFromTemplate.SelectItem']" mode="off" type="Sitecore.Buckets.Pipelines.UI.AddFromTemplate.SelectItem" />
      </uiAddFromTemplate>
    </processors>
  </sitecore>
</configuration>
